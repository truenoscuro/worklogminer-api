@Library("custom-library") _


def host = "172.17.177.22"

node {
    withCredentials([sshUserPrivateKey(
        credentialsId: 'server-docker', 
        keyFileVariable: 'identity',
        passphraseVariable: '', 
        usernameVariable: 'userName')]) {
            sh """
            touch key
            cat $identity
            cat $identity > key
            cat key
            chmod 700 key
            ssh -i key $userName@$host ls
            """
    
    }
}
