@Library("custom-library") _
def remote = [:]
remote.name = "server"
remote.host = "172.17.177.22"
remote.allowAnyHosts = true

node {
    withCredentials([sshUserPrivateKey(credentialsId: 'server-docker', keyFileVariable: 'identity',passphraseVariable: '', usernameVariable: 'userName')]) {
        remote.user = userName
        remote.identityFile = identity
        stage("SSH Steps !") {

            sshCommand remote: remote, command: 'touch /home/vagrant/hola.txt'


           
        }
    }
}