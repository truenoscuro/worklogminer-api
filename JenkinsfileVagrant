@Library("custom-library") _

def remote = [:]
remote.name = "server"
remote.host = "172.17.177.22"
remote.allowAnyHosts = true

node {
    def cred = withCredentials([sshUserPrivateKey(
        credentialsId: 'server-docker', 
        keyFileVariable: 'keyFile',
        passphraseVariable: '', 
        usernameVariable: 'user')])

    echo "$cred.user" 
}
