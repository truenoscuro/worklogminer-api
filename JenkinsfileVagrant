@Library("custom-library") _

def remote = [:]
remote.name = "server"
remote.host = "172.17.177.22"
remote.allowAnyHosts = true
def scriptC = { String c ->  sshCommand([remote: this.remote , command: "${c}",sudo: false])}
def put = { String f,String i ->  sshPut ([remote: this.remote , from: "${f}",into:"${i}",sudo: false])}
def get = { String f,String i ->  sshPut ([remote: this.remote , from: "${f}",into:"${i}",sudo: false, override: true])}
def remove = { String p ->  sshPut ([remote: remote , path: "${p}",sudo: false])}
node {
    withCredentials([sshUserPrivateKey(
        credentialsId: 'server-docker', 
        keyFileVariable: 'keyFile',
        passphraseVariable: '', 
        usernameVariable: 'userName')]) {

        remote.user =  userName
        remote.identityFile = keyFile 
        funct("ls")
    }
    

}
